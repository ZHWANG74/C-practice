int main()
{
    vector<string> V= {"www", "oee", "rid", "ni", "snis"};
    sort(V.begin(), V.end());
    for(int i = 0; i < V.size(); ++i)
        cout << V[i] << " ";
    cout << endl;
    
    sort(V.rbegin(), V.rend()); //逆向排序
    for(int i = 0; i < V.size(); ++i)
        cout << V[i] << " ";
    cout << endl;
    
    for(string& x:V)
        cout << x << " ";
    cout << endl;
        
    for(const auto& x:V)
        cout << x << " ";
    cout << endl;
        
}
